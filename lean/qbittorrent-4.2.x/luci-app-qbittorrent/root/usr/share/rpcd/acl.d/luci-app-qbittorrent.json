{
	"luci-app-qbittorrent": {
		"description": "Grant access to LuCI app qBittorrent",
		"read": {
			"ubus": {
				"network.interface.lan": [ "status" ],
				"service": [ "list" ],
				"uci": [ "get" ]
			},
			"file": {
				"/etc/group": [ "read" ],
				"/etc/passwd": [ "read" ],
				"/etc/init.d/qbittorrent": [ "exec" ],
				"/usr/bin/qbittorrent-nox": [ "exec" ]
			},
			"uci": [ "qbittorrent" ]
		},
		"write": {
			"file": {
				"/bin/rm": [ "exec" ]
			},
			"uci": [ "qbittorrent" ]
		}
	}
}
